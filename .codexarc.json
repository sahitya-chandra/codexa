{
  "modelProvider": "groq",
  "model": "llama-3.1-8b-instant",
  "embeddingProvider": "local",
  "embeddingModel": "Xenova/all-MiniLM-L6-v2",
  "maxChunkSize": 800,
  "chunkOverlap": 100,
  "includeGlobs": [
    "**/*.cjs",
    "**/*.config.js",
    "**/*.config.json",
    "**/*.config.ts",
    "**/*.css",
    "**/*.d.ts",
    "**/*.js",
    "**/*.json",
    "**/*.jsx",
    "**/*.less",
    "**/*.markdown",
    "**/*.md",
    "**/*.mdx",
    "**/*.mjs",
    "**/*.rake",
    "**/*.rb",
    "**/*.sass",
    "**/*.scss",
    "**/*.ts",
    "**/*.tsx",
    "**/.dockerignore",
    "**/.env.example",
    "**/.gitignore",
    "**/CHANGELOG*.md",
    "**/CMakeLists.txt",
    "**/CONTRIBUTING*.md",
    "**/Cargo.toml",
    "**/Gemfile",
    "**/LICENSE*",
    "**/Makefile",
    "**/README*.md",
    "**/README*.txt",
    "**/build.gradle*",
    "**/composer.json",
    "**/go.mod",
    "**/package.json",
    "**/pom.xml",
    "**/pubspec.yaml",
    "**/pyproject.toml",
    "**/requirements.txt",
    "**/tsconfig*.json"
  ],
  "excludeGlobs": [
    "**/*.bundle.js",
    "**/*.chunk.js",
    "**/*.d.ts.map",
    "**/*.gem",
    "**/*.log",
    "**/*.min.js",
    "**/*.swo",
    "**/*.swp",
    "**/*~",
    "**/.DS_Store",
    "**/.bundle/**",
    "**/.byebug_history",
    "**/.cache/**",
    "**/.idea/**",
    "**/.temp/**",
    "**/.tmp/**",
    "**/.vs/**",
    "**/.vscode/**",
    "**/Thumbs.db",
    "**/coverage/**",
    "**/log/**",
    "**/tmp/**",
    ".codexa/**",
    ".codexarc.json",
    ".git/**",
    ".next/**",
    ".nuxt/**",
    ".nyc_output/**",
    ".output/**",
    ".vitepress/dist/**",
    ".vuepress/dist/**",
    "build/**",
    "coverage/**",
    "dist/**",
    "node_modules/**",
    "package-lock.json",
    "pnpm-lock.yaml",
    "vendor/bundle/**",
    "yarn.lock"
  ],
  "historyDir": ".codexa/sessions",
  "dbPath": ".codexa/index.db",
  "temperature": 0.2,
  "topK": 10,
  "maxFileSize": 5242880,
  "skipBinaryFiles": true,
  "skipLargeFiles": true,
  "groqApiKey": "test_key_123"
}
